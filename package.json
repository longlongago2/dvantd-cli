{
  "name": "dvantd-cli",
  "version": "0.2.1",
  "description": "The command-line utility based on dva and antd",
  "bin": {
    "dvantd": "./bin/dvantd"
  },
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/longlongago2/dvantd-cli.git"
  },
  "keywords": [
    "dva",
    "antd"
  ],
  "author": "zhanggl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/longlongago2/dvantd-cli/issues"
  },
  "homepage": "https://github.com/dvantd/dvantd-cli/README.md",
  "babel": {
    "minified": true,
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-runtime",
      "remove-comments"
    ]
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "empty-dir": "^0.2.1",
    "fs-extra": "^1.0.0",
    "left-pad": "^1.1.3",
    "through2": "^2.0.3",
    "vinyl-fs": "^2.4.4",
    "which": "^1.2.12",
    "win-spawn": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.18.0"
  },
  "files": [
    "lib",
    "bin",
    "src",
    "boilerplates"
  ]
}
